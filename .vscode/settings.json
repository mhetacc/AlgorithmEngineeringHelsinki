{
    "macros": {
        "italic_latex": [
            {
                "command": "type",
                "args": {
                    "text": "\\textit{}"
                }
            },
            "cursorLeft"
        ],
        "latex_code_word": [
            {
                "command": "type",
                "args": {
                    "text": "\\verb++"
                }
            },
            "cursorLeft"
        ],
        "markdown_code_block": [
            {
                "command": "type",
                "args": {
                    "text": "```python```"
                }
            },
            "cursorLeft",
            "cursorLeft",
            "cursorLeft"
        ]

},
"editor.wordWrap": "on",
"cSpell.enabled": true,
"cSpell.words": [
    "asyncio",
    "automagically",
    "gameplay",
    "interactability",
    "interactable",
    "linearizability",
    "nonlocal",
    "Pygame",
    "Raftian",
    "subclassing",
    "Tkinter"
],
"latex-workshop.latex.outDir": "%WORKSPACE_FOLDER%/build",
"latex-workshop.latex.autoBuild.run": "never"
}
 